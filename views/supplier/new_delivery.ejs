<!DOCTYPE html>
<html lang="en">

  <head>
    <%- include ("partials/head.ejs") %>
  </head>
  
  <body>
  
<div class="site-wrap">
  
  <%- include ("partials/header.ejs") %>

    <div class="bg-light py-3">
      <div class="container">
        <div class="row">
          <div class="col-md-12 mb-0">
            <a href="/supplier/deliveries">Deliveries</a> <span class="mx-2 mb-0">/</span>
            <strong class="text-black">New Delivery</strong>
          </div>
        </div>
      </div>
    </div>

    <div class="site-section">
      <div class="container">
        <div class="row">
          <!-- <div class="col-md-12">
            <h2 class="h3 mb-5 text-black">Log In</h2>
          </div> -->
          <div class="col-md-12">
            <div class="">
                <form action="/supplier/new_delivery" method="post">
                    <div class="form-group">
                      <label for="exampleInputEmail1">Supplier</label>
                      <input name="supplier" type="text" class="form-control" value="<%= delivery %>" readonly required>
                    </div>
                    <div class="form-group">
                        <label for="exampleInputEmail1">Batch Id</label>
                        <input name="" type="text" class="form-control" id="exampleInputEmail1" value="2378" readonly>
                    </div>
                    <div class="form-group">
                        <label for="exampleInputEmail1">Product Name</label>
                        <input name="title" type="text" class="form-control" id="exampleInputEmail1" placeholder="Product" required>
                    </div>
                    <div class="form-group">
                        <label for="exampleInputPassword1">Quantity (kg)</label>
                        <input name="quantity" type="number" class="form-control" id="exampleInputPassword1" required>
                    </div>
                    <div class="form-group">
                        <label for="exampleInputPassword1">Total</label>
                        <input name="amount" type="number" class="form-control" id="exampleInputPassword1" required>
                    </div>
                    <div class="form-group">
                      <label for="exampleInputPassword1">Expiry Date</label>
                      <input name="expiry_date" type="date" class="form-control" id="exampleInputPassword1" required>
                  </div>
                  
                    <div class="form-group">
                        <input type="submit" class="btn btn-success" value="Submit">
                    </div>
                </form>
            </div>
            
          </div>
          
        </div>
      </div>
    </div>



    <div class="bg-primary">
        <%- include ("partials/footer.ejs") %>
    </div>

</div>

  <script src="js/jquery-3.3.1.min.js"></script>
  <script src="js/jquery-ui.js"></script>
  <script src="js/popper.min.js"></script>
  <script src="js/bootstrap.min.js"></script>
  <script src="js/owl.carousel.min.js"></script>
  <script src="js/jquery.magnific-popup.min.js"></script>
  <script src="js/aos.js"></script>

  <script src="js/main.js"></script>

</body>

</html>

